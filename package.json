    "smoke": "node test/e2e-smoketest/server.js"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "keywords": [
    "socket.io",
    "rpc",
    "model",
    "liveQuery",
    "realtime",
    "mongo",
    "mongoDB",
    "isomorphic",
    "universal",
    "mean",
    "meteor",
    "angular",
    "aurelia",
    "orm",
    "mongoose",
    "mongoosejs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/capaj/Moonridge.git"
  },
  "homepage": "http://capaj.github.io/Moonridge/",
  "dependencies": {
    "baucis": "^1.6.3",
    "baucis-swagger": "^1.1.2",
    "debug": "^2.2.0",
    "express": "^4.13",
    "lodash": "^3.9",
    "mongoose": "^4.2.9",
    "socket.io": "~1.3",
    "socket.io-rpc": "^1.0.2"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "husky": "^0.10.2",
    "mocha": "^2.3.3",
    "mongojs": "^2.0.0",
    "morgan": "^1.6.1",
    "socket.io-rpc-client": "^1.0.2",
    "standard": "^5.4.1"
